{
  "name": "moowi-installer",
  "version": "4.5.0",
  "description": "CoreUI Free React Admin Template",
  "main": "main.js",
  "homepage": "https://moowigroup.com",
  "bugs": {
    "url": "https://github.com/coreui/coreui-free-react-admin-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:coreui/coreui-free-react-admin-template.git"
  },
  "license": "MIT",
  "author": {
    "name": "Moowi Group",
    "email": "support@moowigroup.com"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "start": "react-scripts start",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand"
  },
  "build": {
    "appId": "assettagging.app.id",
    "productName": "Moowi Installer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "artifactName": "Moowi Installer-${version}-${os}-${arch}-${target}.${ext}",
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "moowi.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Asset Tagging"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.business"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "tar.gz"
      ],
      "maintainer": "Moowi Group <support@moowigroup.com>",
      "executableName": "moowi-installer",
      "vendor": "Moowi Group",
      "desktop": {
        "Name": "Moowi Installer",
        "Comment": "Moowi Asset Tagging",
        "Categories": "Utility;"
      },
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "harry-kuria",
        "repo": "asset-tagging-frontend"
      }
    ]
  },
  "dependencies": {
    "18": "^0.0.0",
    "@coreui/chartjs": "^3.1.2",
    "@coreui/coreui": "^4.2.6",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.1.0",
    "@coreui/react": "^4.9.0-rc.0",
    "@coreui/react-chartjs": "^2.1.3",
    "@coreui/utils": "^2.0.2",
    "@material/linear-progress": "^14.0.0",
    "axios": "^1.6.3",
    "chart.js": "^3.9.1",
    "classnames": "^2.3.2",
    "core-js": "^3.31.0",
    "cors": "^2.8.5",
    "electron-log": "^5.1.4",
    "electron-updater": "^6.1.8",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.2.0",
    "jspdf-autotable": "^3.8.1",
    "pdf-lib": "^1.17.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-app-polyfill": "^3.0.0",
    "react-barcode": "^1.4.6",
    "react-bootstrap": "^2.9.2",
    "react-chartjs-2": "^4.3.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.30.1",
    "react-to-pdf": "^1.0.1",
    "react-toastify": "^9.1.3",
    "redux": "4.2.1",
    "simplebar-react": "^2.4.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron/packager": "^18.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "electron": "^28.1.4",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.8",
    "react-scripts": "5.0.1",
    "sass": "^1.63.6",
    "web-vitals": "^3.3.2"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
